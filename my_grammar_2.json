{
    "start"     : "START",
    "terms"     :
    [
        "ident", "int_const", "float_const", "func_lex", "if_lex", "else_lex",
        "for_lex", "range_lex", "case_lex", "break_lex", "default_lex", "switch_lex",
        "var_lex", "bool_lex", "int_lex", "float_lex", "string_lex", "return_lex",
        "string_const", "and", "or", "greater_or_eq", "less_or_eq", "not_equal",
        "is_equal", "assing", "plus_eq", "minus_eq", "div_eq", "mod_eq", "mul_eq",
        "plus", "minus", "mult", "div", "mod", "equating", "greater", "less",
        "not", "inc", "dec", "open_parentheses",
        "close_parentheses", "open_bracket", "close_bracket", "semicolon",
        "colon", "comma", "point", "open_brace", "close_brace", "print", "scan", "pow", "main_lex", "bool_const",
        "in_lex"
    ],

    "rules"     : {
        "START" : [
            ["PROG", "end"]
        ],

        "PROG" : [
            ["MAIN_FUNCTION"],
            ["MAIN_FUNCTION", "FUNCTIONS"],
            ["FUNCTIONS", "MAIN_FUNCTION"],
            ["FUNCTIONS", "MAIN_FUNCTION", "FUNCTIONS"]
        ],

        "FUNCTIONS" : [
            ["FUNCTION"],
            ["FUNCTIONS", "FUNCTION"]
        ],

        "FUNCTION" : [
            ["func_lex", "ident", "open_parentheses", "INPUT_PARAMS", "close_parentheses", "TYPE_WITH_VOID", "OPERATORS_BLOCK"]
        ],

        "MAIN_FUNCTION" : [
            ["func_lex", "main_lex", "open_parentheses", "close_parentheses", "OPERATORS_BLOCK"]
        ],

        "TYPE_WITH_VOID" : [
            [],
            ["TYPE"]
        ],

        "TYPE" : [
            ["int_lex"],
            ["float_lex"],
            ["string_lex"],
            ["bool_lex"]
        ],

        "INPUT_PARAMS" : [
            [],
            ["NOTVOID_INPUT_PARAMS"]
        ],

        "NOTVOID_INPUT_PARAMS" : [
            ["INPUT_PARAM"],
            ["NOTVOID_INPUT_PARAMS", "comma", "INPUT_PARAM"]
        ],

        "INPUT_PARAM" : [
            ["ident", "TYPE"]
        ],

        "OPERATORS" : [
            [],
            ["OPERATORS", "OPERATOR"]
        ],

        "OPERATOR" : [
            ["FUNCTION_CALL"],
            ["EQUATE"],
            ["CYCLES"],
            ["IF"],
            ["OPERATORS_BLOCK"],
            ["VAR_DECL"],
            ["return_lex", "EXPR"]
        ],

        "OPERATORS_BLOCK" : [
            ["open_brace", "OPERATORS", "close_brace"]
        ],

        "FUNCTION_CALL" : [
            ["ident", "open_parentheses", "PARAMS", "close_parentheses"],
            ["print", "open_parentheses", "PARAMS", "close_parentheses"],
            ["scan", "open_parentheses", "PARAMS", "close_parentheses"]
        ],

        "PARAMS" : [
            [],
            ["NOTVOID_PARAMS"]
        ],

        "NOTVOID_PARAMS" : [
            ["EXPR"],
            ["NOTVOID_PARAMS", "comma", "EXPR"]
        ],

        "EQUATE" : [
            ["ident", "equating", "EXPR"]
        ],

        "CYCLES" : [
            ["FOR"],
            ["WHILE"],
            ["FOR_IN"]
        ],

        "FOR" : [
            ["for_lex", "FOR_INIT", "semicolon", "EXPR", "semicolon", "FOR_ITER", "OPERATORS_BLOCK"]
        ],

        "WHILE" : [
            ["for_lex", "EXPR", "OPERATORS_BLOCK"]
        ],

        "FOR_IN" : [
            ["for_lex", "ident", "in_lex", "ident", "OPERATORS_BLOCK"]
        ],

        "FOR_INIT" : [
            [],
            ["EQUATE"],
            ["EXPR"],
            ["VAR_DECL"]
        ],

        "FOR_ITER" : [
            [],
            ["FUNCTION_CALL"],
            ["EQUATE"]
        ],

        "IF" : [
            ["if_lex", "EXPR", "OPERATORS_BLOCK"],
            ["if_lex", "EXPR", "OPERATORS_BLOCK", "else_lex", "OPERATORS_BLOCK"]
        ],

        "VAR_DECL" : [
            ["var_lex", "IDENT_LIST", "TYPE"]
        ],

        "IDENT_LIST":[
            ["ident"],
            ["IDENT_LIST", "comma", "ident"]
        ],

        "EXPR0" : [
            ["ident"],
            ["FUNCTION_CALL"],
            ["int_const"],
            ["float_const"],
            ["bool_const"],
            ["string_const"],
            ["open_parentheses", "EXPR", "close_parentheses"],
            ["not", "EXPR0"],
            ["plus", "EXPR0"],
            ["minus", "EXPR0"]
        ],

        "EXPR1" : [
            ["EXPR0"],
            ["EXPR1", "mult", "EXPR0"],
            ["EXPR1", "div", "EXPR0"],
            ["EXPR1", "mod", "EXPR0"]
        ],

        "EXPR2" : [
            ["EXPR1"],
            ["EXPR2", "plus", "EXPR1"],
            ["EXPR2", "minus", "EXPR1"]
        ],

        "EXPR3" : [
            ["EXPR2"],
            ["EXPR3", "greater_or_eq", "EXPR2"],
            ["EXPR3", "less_or_eq", "EXPR2"],
            ["EXPR3", "not_equal", "EXPR2"],
            ["EXPR3", "is_equal", "EXPR2"],
            ["EXPR3", "greater", "EXPR2"],
            ["EXPR3", "less", "EXPR2"]
        ],

        "EXPR4" : [
            ["EXPR3"],
            ["EXPR4", "and", "EXPR3"]
        ],

        "EXPR" : [
            ["EXPR4"],
            ["EXPR", "or", "EXPR4"]
        ]
    }
}